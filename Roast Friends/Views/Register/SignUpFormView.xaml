<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Roast_Friends.Views.Register.SignUpFormView"
             Title="SignUpFormView">
    <ContentPage.Content>
        <StackLayout Padding="20">
            <Entry x:Name="EmailEntry" Placeholder="Email" Keyboard="Email"/>
            <Entry x:Name="PasswordEntry" Placeholder="Hasło" IsPassword="true"/>
            <Entry x:Name="ConfirmPasswordEntry" Placeholder="Potwierdź hasło" IsPassword="true"/>
            <Entry x:Name="LoginEntry" Placeholder="Login"/>
            <Button Text="Zarejestruj się" Clicked="RegisterButton_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>